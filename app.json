{
  "expo": {
    "name": "Secret Camera",
    "description": "a app to use camera in secret",
    "slug": "secret-camera",
    "privacy": "public",
    "sdkVersion": "22.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icons/app-icon.png",
    "loading": {
      "icon": "./assets/icons/loading-icon.png",
      "hideExponentText": false
    },
    "packagerOpts": {
      "assetExts": ["ttf", "mp4"]
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryAddUsageDescription": "Give <appName> permission to save to your photos"
      },
      "bundleIdentifier": "com.wangdagentest.secretCamera"
    }
  }
}
